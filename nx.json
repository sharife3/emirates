{
  "npmScope": "emirates",
  "affected": {
    "defaultBase": "master"
  },
  "implicitDependencies": {
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    ".eslintrc.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "lint", "test", "e2e"]
      }
    }
  },
  "targetDependencies": {
    "build": [
      {
        "target": "build",
        "projects": "dependencies"
      }
    ]
  },
  "projects": {
    "backend-model": {
      "tags": []
    },
    "common-model": {
      "tags": []
    },
    "flights-gateway": {
      "tags": []
    },
    "flights-service": {
      "tags": []
    },
    "flights-ui": {
      "tags": []
    },
    "flights-ui-e2e": {
      "tags": [],
      "implicitDependencies": ["flights-ui"]
    },
    "frontend-components": {
      "tags": []
    }
  }
}
