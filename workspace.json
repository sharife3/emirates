{
  "version": 2,
  "projects": {
    "flights-gateway": {
      "root": "apps/flights-gateway",
      "sourceRoot": "apps/flights-gateway/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/node:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/flights-gateway",
            "main": "apps/flights-gateway/src/main.ts",
            "tsConfig": "apps/flights-gateway/tsconfig.app.json",
            "assets": ["apps/flights-gateway/src/assets"]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/flights-gateway/src/environments/environment.ts",
                  "with": "apps/flights-gateway/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/node:execute",
          "options": {
            "buildTarget": "flights-gateway:build"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["apps/flights-gateway/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/flights-gateway"],
          "options": {
            "jestConfig": "apps/flights-gateway/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "flights-ui": {
      "root": "apps/flights-ui",
      "sourceRoot": "apps/flights-ui",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/next:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "root": "apps/flights-ui",
            "outputPath": "dist/apps/flights-ui"
          },
          "configurations": {
            "production": {}
          }
        },
        "serve": {
          "executor": "@nrwl/next:server",
          "options": {
            "buildTarget": "flights-ui:build",
            "dev": true
          },
          "configurations": {
            "production": {
              "buildTarget": "flights-ui:build:production",
              "dev": false
            }
          }
        },
        "export": {
          "executor": "@nrwl/next:export",
          "options": {
            "buildTarget": "flights-ui:build:production"
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/flights-ui"],
          "options": {
            "jestConfig": "apps/flights-ui/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["apps/flights-ui/**/*.{ts,tsx,js,jsx}"]
          }
        }
      }
    },
    "flights-ui-e2e": {
      "root": "apps/flights-ui-e2e",
      "sourceRoot": "apps/flights-ui-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/flights-ui-e2e/cypress.json",
            "devServerTarget": "flights-ui:serve",
            "tsConfig": "apps/flights-ui-e2e/tsconfig.json"
          },
          "configurations": {
            "production": {
              "devServerTarget": "flights-ui:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["apps/flights-ui-e2e/**/*.{js,ts}"]
          }
        }
      }
    },
    "shared-ui-components": {
      "root": "libs/shared-ui-components",
      "sourceRoot": "libs/shared-ui-components/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": [
              "libs/shared-ui-components/**/*.{ts,tsx,js,jsx}"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/shared-ui-components"],
          "options": {
            "jestConfig": "libs/shared-ui-components/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/next"
  },
  "generators": {
    "@nrwl/react": {
      "application": {
        "babel": true
      }
    },
    "@nrwl/next": {
      "application": {
        "style": "styled-components",
        "linter": "eslint"
      }
    }
  },
  "defaultProject": "flights-ui"
}
